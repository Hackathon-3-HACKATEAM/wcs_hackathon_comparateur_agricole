Buyercity, setLoadingBuyercity] = useState(false);\n\n  useEffect(() => {\n    if (\"geolocation\" in navigator) {\n      navigator.geolocation.getCurrentPosition(function (position) {\n        setLatitude(position.coords.latitude);\n        setLongitude(position.coords.longitude);\n      });\n    } else {\n      setAvaibleGPS(false);\n    }\n  }, []);\n\n  useEffect(() => {\n    axios\n      .get(`${FETCH}/data?size=`)\n      .then((res) => {\n        setDatas(res.data);\n        setIsLoadingData(true);\n      })\n      .catch(function (erreur) {\n        console.log(erreur);\n      });\n  }, []);\n\n  useEffect(() => {\n    axios\n      .get(`${FETCH}/buyerscity`)\n      .then((res) => {\n        setBuyercity(res.data);\n        setLoadingBuyercity(true);\n      })\n      .catch(function (erreur) {\n        console.log(erreur);\n      });\n  }, []);\n\n  const positionQG = [48.4470213, 1.5375294];\n  const localisationCircle = { color: \"#5a9449\" };\n\n  return lat && long ? (\n    <MapContainer\n      className=\"map\"\n      center={[lat, long]}\n      zoom={13}\n      scrollWheelZoom={true}\n      wheelPxPerZoomLevel={400}\n    >\n      <TileLayer\n        attribution='&copy; <a href=\"http://osm.org/copyright\">OpenStreetMap</a> contributors'\n        url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\n      />\n\n      <LayerGroup>\n        <Circle\n          center={[lat, long]}\n          pathOptions={localisationCircle}\n          radius={800}\n          className=\"circle-localisation\"\n        />\n      </LayerGroup>\n\n      <Marker position={positionQG} icon={qgIcon}>\n        <Popup className=\"popup\">\n          <h3 className=\"popup-locaux\">Locaux de ComparateurAgricole</h3>\n        </Popup>\n      </Marker>\n\n      {isLoadingData ? \n        datas.slice(0, 1000).map((data, index) => (\n            <Marker position={[data.lat, data.long]} icon={tractorIcon}>\n              <Popup className=\"popup-farmer\">\n                    <Popup_component infos={data} />\n              </Popup>\n            </Marker>\n        ))\n        : null\n      }\n\n      {isLoadingBuyercity\n        ? buyercity.slice(0, 8).map((res) => (\n            <Marker position={[res.lat, res.long]} icon={buyerIcon}>\n              <Popup className=\"popup-buyer\">\n                <h3 className=\"popup-locaux\">{res.name}</h3>\n              </Popup>\n            </Marker>\n          ))\n        : null}\n\n    </MapContainer>\n  ) : avaibleGPS ? (\n    <p className=\"wait-localisation\">Veuillez activer la géolocalisation</p>\n  ) : (\n    <p className=\"no-localisation\">\n      Désolé... Votre navigateur ne dispose pas d'un système de géolocalisation\n    </p>\n  );\n}\n",["64","65"],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/chat/ChatIcon.jsx",[],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/interactive-map/pop_up_component/popup-component.jsx",["66"],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/index.jsx",[],["67","68"],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/Fetch.js",[],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/chat/JoinChat.jsx",[],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/accordion/AccordionContainer.jsx",["69"],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/App.jsx",["70"],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/accordion/AccordionIcon.jsx",[],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/chat/ChatRoom.jsx",["71"],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/logo-svg/LogoSVG.jsx",[],["72","73"],"/home/florian/Bureau/Hackton3/wcs_hackathon_comparateur_agricole/client/src/components/chat/ChatContainer.jsx",[],"/home